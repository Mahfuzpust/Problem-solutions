#include<stdio.h>
int main()
{
    int t,T,i;
    int a,b;
    scanf("%d",&T);
    for(t=0;t<T;t++)
    {
        scanf("%d%d",&a,&b);
        int s=0;
        for(i=a;i<=b;i++)
        {
            if(i%2==1)
                s=s+i;
        }
        printf("Case %d: %d\n",t+1,s);
    }
   return 0;
}
